<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="matheus.senai.projetocantina">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconeapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconeapp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MostraProduto"></activity>
        <activity android:name=".MostraCardapio" />
        <activity android:name=".MainActivity" />
        <activity android:name=".TelaCardapio" />
        <activity
            android:name=".splash_screen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>